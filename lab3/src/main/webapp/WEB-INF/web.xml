<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
                   version="4.0">

  <filter>
    <filter-name>Filter</filter-name>
    <filter-class>filter.Filter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Filter</filter-name>
    <url-pattern>*</url-pattern>
  </filter-mapping>

  <servlet>
  <servlet-name>ServletSignIn</servlet-name>
  <servlet-class>servlets.ServletSignIn</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>ServletSignIn</servlet-name>
  <url-pattern>/ServletSignIn</url-pattern>
</servlet-mapping>

  <servlet>
    <servlet-name>ServletSignUp</servlet-name>
    <servlet-class>servlets.ServletSignUp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletSignUp</servlet-name>
    <url-pattern>/ServletSignUp</url-pattern>
  </servlet-mapping>
</web-app>
